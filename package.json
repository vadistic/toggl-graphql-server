{
  "name": "toggl-graphql-server",
  "version": "0.1.0",
  "description": "GraphQL API for [toggl](https://toggl.com) rest api. [ENDPOINT](https://toggl-graphql-server.vadistic.now.sh) [DOCS](https://toggl-graphql-server.vadistic.now.sh/docs/)",
  "main": "src/index.ts",
  "repository": "https://github.com/vadistic/toggl-graphql-server",
  "homepage": "https://toggl-graphql-server.vadistic.now.sh",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "micro ./build/index.js",
    "dev": "node -r ts-node/register node_modules/.bin/micro-dev ./src/index.ts",
    "build": "tsc -b",
    "gen:doc": "graphdoc --force -e http://localhost:3000 -o ./docs",
    "gen:gql": "graphql-codegen --config codegen.yml",
    "gen": "yarn gen:doc && yarn gen:gql"
  },
  "dependencies": {
    "@graphql-modules/core": "^0.7.12",
    "apollo-datasource-rest": "^0.6.4",
    "apollo-server-micro": "^2.9.3",
    "base-64": "^0.1.0",
    "graphql": "^14.5.6",
    "micro": "^9.3.4",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@2fd/graphdoc": "^2.4.0",
    "@graphql-codegen/cli": "^1.7.0",
    "@graphql-codegen/introspection": "^1.7.0",
    "@graphql-codegen/typescript": "1.7.0",
    "@graphql-codegen/typescript-document-nodes": "1.7.0",
    "@graphql-codegen/typescript-resolvers": "1.7.0",
    "@types/base-64": "^0.1.3",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^24.0.18",
    "@types/micro": "^7.3.3",
    "@types/node": "^12.7.5",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.9.0",
    "micro-dev": "^3.0.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  }
}
