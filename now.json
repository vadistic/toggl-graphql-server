{
  "version": 2,
  "builds": [
    { "src": "src/index.ts", "use": "@now/node" },
    { "src": "docs/**/*", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/docs/(.*)", "dest": "/docs/$1" },
    {
      "src": "/github",
      "status": 301,
      "headers": { "Location": "https://github.com/vadistic/toggl-graphql-server" }
    },
    { "src": "/(.*)", "dest": "/src/index.ts" }
  ],
  "public": true
}
